<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.administrator.stock.ui.activity.SetActivity"
    android:background="#f8f8f8">
<LinearLayout
    android:id="@+id/ll_status_activity_set"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"></LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="44px"
        android:background="#ff0000">
        <ImageView
            android:id="@+id/img_return_activity_set"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/all_return"
            android:padding="12px"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置中心"
            android:textColor="#FFFFFFFF"
            android:textSize="17px"
            android:layout_centerInParent="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_phone_activity_set"
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:layout_marginTop="8px"
        android:background="#ffffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="更改手机"
            android:textColor="#FF323232"
            android:textSize="14px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:layout_width="9px"
            android:layout_height="16px"
            android:src="@drawable/all_next"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_font_activity_set"
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:layout_marginTop="1px"
        android:background="#ffffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="更改字号"
            android:textColor="#FF323232"
            android:textSize="14px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:layout_width="9px"
            android:layout_height="16px"
            android:src="@drawable/all_next"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_feedback_activity_set"
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:layout_marginTop="1px"
        android:background="#ffffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="用户反馈"
            android:textColor="#FF323232"
            android:textSize="14px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:layout_width="9px"
            android:layout_height="16px"
            android:src="@drawable/all_next"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:layout_marginTop="1px"
        android:background="#ffffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="当前版本"
            android:textColor="#FF323232"
            android:textSize="14px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
       <TextView
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="V1.0.1"
           android:textColor="#40121212"
           android:textSize="14px"
           android:layout_centerVertical="true"
           android:layout_alignParentRight="true"
           android:layout_marginRight="12px"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_about_activity_set"
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:layout_marginTop="1px"
        android:background="#ffffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="关于我们"
            android:textColor="#FF323232"
            android:textSize="14px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:layout_width="9px"
            android:layout_height="16px"
            android:src="@drawable/all_next"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_skin_activity_set"
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:layout_marginTop="1px"
        android:background="#ffffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="皮肤设置"
            android:textColor="#FF323232"
            android:textSize="14px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:layout_width="9px"
            android:layout_height="16px"
            android:src="@drawable/all_next"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_cache_activity_set"
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:layout_marginTop="1px"
        android:background="#ffffff">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="清除缓存"
            android:textColor="#FF323232"
            android:textSize="14px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:layout_width="9px"
            android:layout_height="16px"
            android:src="@drawable/all_next"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
    </RelativeLayout>
<RelativeLayout
    android:id="@+id/rl_logout_activity_set"
    android:layout_width="match_parent"
    android:layout_height="48px"
    android:layout_marginTop="13px"
    android:background="#ffffff">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="退出登录"
        android:textColor="#FFFF0000"
        android:textSize="16px"
        android:gravity="center"
        android:layout_centerInParent="true"/>
</RelativeLayout>
</LinearLayout>
