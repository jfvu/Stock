<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.administrator.stock.ui.activity.ReleaseActivity"
    android:background="#f8f8f8">
<LinearLayout
    android:id="@+id/ll_status_activity_release"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"></LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="44px"
        android:background="#ff0000">
        <RelativeLayout
            android:id="@+id/rl_return_activity_release"
            android:layout_width="35px"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="13px"
                android:layout_height="22px"
                android:layout_centerInParent="true"
                android:src="@drawable/all_return"/>
        </RelativeLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="发布公式"
            android:textColor="#FFFFFFFF"
            android:textSize="17px"
            android:layout_centerInParent="true"/>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="#ffffff"
        android:layout_marginTop="8px">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="参数名称："
            android:textColor="#E6121212"
            android:textSize="16px"
            android:layout_marginLeft="12px"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="XXXXXX"
            android:textColor="#E6121212"
            android:textSize="14px"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_versions"
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="#ffffff"
        android:layout_marginTop="1px">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="参数版本："
            android:textColor="#E6121212"
            android:textSize="16px"
            android:layout_marginLeft="12px"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="1.0"
            android:textColor="#E6121212"
            android:textSize="14px"/>
        <ImageView
            android:layout_width="8px"
            android:layout_height="14px"
            android:layout_marginRight="15px"
            android:src="@drawable/all_next"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="#ffffff"
        android:layout_marginTop="1px">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="参数类型："
            android:textColor="#E6121212"
            android:textSize="16px"
            android:layout_marginLeft="12px"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="XXXXXX"
            android:textColor="#40121212"
            android:textSize="14px"/>
    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="公式描述"
        android:textColor="#FFFF0000"
        android:textSize="14px"
        android:layout_marginTop="8px"
        android:layout_marginLeft="12px"
        android:layout_marginBottom="8px"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="96px"
        android:background="#ffffff"></RelativeLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="审核样例"
        android:textColor="#FFFF0000"
        android:textSize="14px"
        android:layout_marginTop="8px"
        android:layout_marginLeft="12px"
        android:layout_marginBottom="8px"/>
    <LinearLayout
        android:id="@+id/ll_choose"
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="#ffffff"
        android:layout_marginTop="1px">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="选择股票："
            android:textColor="#E6121212"
            android:textSize="16px"
            android:layout_marginLeft="12px"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="XXXXXX"
            android:textColor="#E6121212"
            android:textSize="14px"/>
        <ImageView
            android:layout_width="8px"
            android:layout_height="14px"
            android:layout_marginRight="15px"
            android:src="@drawable/all_next"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_begin"
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="#ffffff"
        android:layout_marginTop="1px">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="起始日期："
            android:textColor="#E6121212"
            android:textSize="16px"
            android:layout_marginLeft="12px"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="XXXXXX"
            android:textColor="#E6121212"
            android:textSize="14px"/>
        <ImageView
            android:layout_width="8px"
            android:layout_height="14px"
            android:layout_marginRight="15px"
            android:src="@drawable/all_next"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_over"
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="#ffffff"
        android:layout_marginTop="1px">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="结束日期："
            android:textColor="#E6121212"
            android:textSize="16px"
            android:layout_marginLeft="12px"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="XXXXXX"
            android:textColor="#E6121212"
            android:textSize="14px"/>
        <ImageView
            android:layout_width="8px"
            android:layout_height="14px"
            android:layout_marginRight="15px"
            android:src="@drawable/all_next"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_price"
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="#ffffff"
        android:layout_marginTop="8px">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="购买价格："
            android:textColor="#E6121212"
            android:textSize="16px"
            android:layout_marginLeft="12px"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="金币，120"
            android:textColor="#E6121212"
            android:textSize="14px"/>
        <ImageView
            android:layout_width="8px"
            android:layout_height="14px"
            android:layout_marginRight="15px"
            android:src="@drawable/all_next"/>
    </LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="48px"
            android:layout_alignParentBottom="true"
            android:background="@drawable/buy_1"
            android:orientation="horizontal">
            <LinearLayout
                android:id="@+id/ll_single"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#ff0000"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="13px"
                    android:layout_height="12px"
                    android:src="@drawable/history_1"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="个股测试"
                    android:textColor="#FFFFFFFF"
                    android:textSize="14px"
                    android:layout_marginLeft="3px"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/ll_overall"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:gravity="center">
                <ImageView
                    android:layout_width="12px"
                    android:layout_height="12px"
                    android:src="@drawable/history_2"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="大盘测试"
                    android:textColor="#FFFF0000"
                    android:textSize="14px"
                    android:layout_marginLeft="3px"/>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
